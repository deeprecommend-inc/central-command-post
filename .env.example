# BrightData Proxy Settings
BRIGHTDATA_USERNAME=your_username
BRIGHTDATA_PASSWORD=your_password
BRIGHTDATA_HOST=brd.superproxy.io
BRIGHTDATA_PORT=22225
# Proxy type: residential (住宅IP - デフォルト), datacenter, mobile, isp
BRIGHTDATA_PROXY_TYPE=residential

# Browser Settings
HEADLESS=true
PARALLEL_SESSIONS=5

# OpenAI API (for browser-use)
OPENAI_API_KEY=your_openai_api_key

# Anthropic API (for Claude models)
ANTHROPIC_API_KEY=your_anthropic_api_key

# CAPTCHA Solver (2Captcha or Anti-Captcha)
CAPTCHA_PROVIDER=2captcha
CAPTCHA_API_KEY=your_captcha_api_key

# Redis (optional, for distributed mode)
REDIS_URL=redis://localhost:6379
